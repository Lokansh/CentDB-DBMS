[students]
id int(20),
name varchar(25),
address varchar(20),
PRIMARY KEY (id),
FOREIGN KEY (FKPersonId) REFERENCES testTable11(PersonID),
FOREIGN KEY (FKPersonId2) REFERENCES testTable12(PersonID);

[testTable11]
PersonID int,
Personname varchar,
PRIMARY KEY (PersonID);

[testTable12]
PersonID int,
Personname varchar,
PRIMARY KEY (PersonID);